{
  "name": "routica",
  "displayName": "Routica",
  "description": "Routica is a VS Code extension for Node.js and Express developers. It lets you test and analyze Express controller functions directly in the editor, simplifying debugging and enhancing productivity. With its intuitive interface, Routica streamlines workflows, making development faster and more efficient.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:typescript"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "routica.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "routica.activate",
        "title": "Activate Routica"
      },
      {
        "command": "routica.analyzeProject",
        "title": "Routica: Analyze Project"
      },
      {
        "command": "routica.showDate",
        "title": "Routica: Show Date"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@types/vscode": "^1.95.0",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.13.0",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@typescript-eslint/typescript-estree": "^8.15.0",
    "acorn": "^8.14.0",
    "acorn-typescript": "^1.4.13",
    "acorn-walk": "^8.3.4"
  }
}
